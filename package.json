    "serve": "NODE_OPTIONS='--experimental-strip-types --import @greenwood/cli/register' greenwood serve",
    "start": "npm run clean && npm run build && npm run serve",
    "lint": "tsc --project tsconfig.json"
  },
  "devDependencies": {
    "@greenwood/cli": "^0.32.0-alpha.3",
    "@greenwood/plugin-adapter-vercel": "^0.32.0-alpha.3",
    "@greenwood/plugin-css-modules": "^0.32.0-alpha.3",
    "@greenwood/plugin-import-raw": "^0.32.0-alpha.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.0"
  }
}
