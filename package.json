{
  "name": "greenwood-native-typescript",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./public .greenwood/",
    "dev": "NODE_OPTIONS='--experimental-strip-types --import @greenwood/cli/register' greenwood develop",
